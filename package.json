{
  "name": "ember-percy",
  "version": "1.2.22",
  "description": "Ember addon for visual regression testing with Percy (https://percy.io).",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment test",
    "start": "ember server",
    "pretest": "bower cache clean && bower install",
    "test": "ember try:testall",
    "test:one": "ember try:one ember-release",
    "test:node_debug": "node debug node_modules/ember-cli/bin/ember test"
  },
  "repository": "https://github.com/percy/ember-percy",
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "author": "Perceptual Inc.",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "^2.16.2",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^4.2.0",
    "ember-cli-htmlbars": "^1.0.10",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^3.0.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.13.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-fetch": "^3.4.0",
    "ember-load-initializers": "^0.5.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-event-dispatcher": "^0.6.3",
    "ember-native-dom-helpers": "^0.5.4",
    "ember-resolver": "^2.0.3",
    "ember-source": "~2.13.0",
    "eslint-plugin-ember": "^4.5.0",
    "loader.js": "^4.0.10"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "body-parser": "^1.15.0",
    "bower": "1.8.2",
    "ember-cli-babel": "^6.8.2",
    "ember-cli-babel": "^6.10.0",
    "es6-promise-pool": "^2.4.1",
    "global": "^4.3.2",
    "percy-client": "^2.4.2",
    "walk": "^2.3.9"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
